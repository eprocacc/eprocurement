<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity SupplierGeneralInfo.
    -->
    <changeSet id="20180205121049-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="supplier_id"
                                 baseTableName="supplier_general_info"
                                 constraintName="fk_supplier_general_info_supplier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="supplier"/>

        <addForeignKeyConstraint baseColumnNames="supplier_general_to_address_id"
                                 baseTableName="supplier_general_info"
                                 constraintName="fk_supplier_general_info_supplier_general_to_address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address"/>

        <addForeignKeyConstraint baseColumnNames="general_infos_to_supplier_id"
                                 baseTableName="supplier_general_info"
                                 constraintName="fk_supplier_general_info_general_infos_to_supplier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="supplier"/>

    </changeSet>
</databaseChangeLog>
